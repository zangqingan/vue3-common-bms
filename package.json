    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "3.4.2",
    "rimraf": "^6.0.1",
    "stylelint": "^16.14.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^6.0.0",
