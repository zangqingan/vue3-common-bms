    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
